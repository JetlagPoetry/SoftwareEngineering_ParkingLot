<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nku.cs.parkinglot.BookResultFeedbackActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="200dp"
        android:layout_height="36dp"
        android:text="预定成功"
        android:textAlignment="center"
        android:gravity="center"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="12dp"
        android:layout_marginLeft="12dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="12dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="100dp"
        android:layout_height="24dp"
        android:text="订单详情"
        android:gravity="center"
        android:textAlignment="center"
        android:textSize="14sp"
        android:layout_marginTop="39dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:layout_marginLeft="12dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="12dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="40dp"
        android:text="订单类型"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.875" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginLeft="24dp"
        android:text="停车场"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginLeft="24dp"
        android:text="起始时间"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginLeft="24dp"
        android:text="结束时间"
        app:layout_constraintBottom_toTopOf="@+id/textView13"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="120dp"
        android:layout_marginLeft="24dp"
        android:text="天数"
        app:layout_constraintBottom_toTopOf="@+id/btn_BookFinish"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        android:text="金额"
        app:layout_constraintBaseline_toBaselineOf="@+id/txv_EndTime"
        app:layout_constraintLeft_toRightOf="@+id/txv_EndTime" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        android:text="支付状态"
        app:layout_constraintBaseline_toBaselineOf="@+id/txv_DayNumber"
        app:layout_constraintLeft_toRightOf="@+id/txv_DayNumber" />

    <Button
        android:id="@+id/btn_BookFinish"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:background="#81d8d0"
        android:text="预约完成"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="12dp"
        android:layout_marginLeft="12dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="12dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        android:text="车牌号"
        app:layout_constraintBaseline_toBaselineOf="@+id/txv_StartTime"
        app:layout_constraintLeft_toRightOf="@+id/txv_StartTime" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        android:text="联系人"
        app:layout_constraintBaseline_toBaselineOf="@+id/txv_BookType"
        app:layout_constraintLeft_toRightOf="@+id/txv_BookType" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        android:text="联系方式"
        app:layout_constraintBaseline_toBaselineOf="@+id/txv_ParkingLot"
        app:layout_constraintLeft_toRightOf="@+id/txv_ParkingLot" />

    <TextView
        android:id="@+id/txv_BookType"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView8"
        app:layout_constraintLeft_toRightOf="@+id/textView8"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/txv_ParkingLot"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView9"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/txv_StartTime"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView10"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/txv_EndTime"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView11"
        app:layout_constraintLeft_toRightOf="@+id/textView11"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/txv_DayNumber"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView13"
        app:layout_constraintLeft_toRightOf="@+id/textView13"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/txv_Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView18"
        android:layout_marginRight="29dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView18"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/txv_Phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView19"
        android:layout_marginRight="29dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView19"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/txv_PlateNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView16"
        android:layout_marginRight="29dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView16"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/txv_Fee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginRight="29dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView14"
        app:layout_constraintLeft_toRightOf="@+id/textView14"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/txv_PayCondition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView15"
        android:layout_marginRight="29dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView15"
        android:layout_marginLeft="12dp" />
</android.support.constraint.ConstraintLayout>
